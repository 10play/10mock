{
  "name": "10mock",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc --project ./",
    "start": "nodemon index.ts",
    "startProxy": "PROXY_URL=http://localhost:5000 nodemon index.ts"
  },
  "files": ["build"],
  "jest": {
    "moduleFileExtensions": [
